<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>OriginalPoster</AssemblyName>
    <RootNamespace>OriginalPoster</RootNamespace>
    <Version>1.0.0</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="mediabrowser.server.core" Version="4.9.1.80" />
  </ItemGroup>

  <ItemGroup>
    <!-- 配置页面HTML文件（第二阶段添加） -->
    <EmbeddedResource Include="Configuration\*.html" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Configuration\config.html" />
  </ItemGroup>

  </Target>

</Project>
